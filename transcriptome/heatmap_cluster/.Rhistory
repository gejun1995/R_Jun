library(pheatmap)
data <- read.table("heatmap RSEM.gene.TMM.EXPR.matrix.csv", sep = ",", header = TRUE)
data <- edit(data)
data <- as.matrix(data)
data <- matrix(as.numeric(data),nrow=nrow(data))
pheatmap(data, color = colorRampPalette(c("blue","white","red"))(256),treeheight_row=50,treeheight_col = 30)
